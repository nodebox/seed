{
    "hosting": {
        "public": "_build",
        "ignore": [
            "firebase.json",
            "**/.*"
        ],
        "rewrites": [
            {
                "source": "/view/**",
                "destination": "/view.html"
            },
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    }
}
